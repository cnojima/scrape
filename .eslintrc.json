{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "func-names": "off",
        "linebreak-style": "off",
        "max-len": "warn",
        "no-console": "warn",
        "no-param-reassign": "off",
        "no-multi-assign": "warn",
        "no-multi-spaces": "warn",
        "no-plusplus": "off"
    }
}
